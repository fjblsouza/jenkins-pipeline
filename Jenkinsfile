pipeline {
	agent any

	stages {
    	stage("Stage-1") {   
        	steps {               	 
            	echo 'hello-World'
        	}    
    	}
    	stage("Stage-2") {   
        	steps {               	 
            	echo 'hello World Again 2'
        	}    
    	}		

    	stage("Stage-3") {   
        	steps {               	 
            	echo 'hello-World Again 3'
        	}    
    	}
    	stage("Stage-4") {   
        	steps {               	 
            	echo 'hello World Again 4'
        	}    
    	}		
    	    	
}
}

